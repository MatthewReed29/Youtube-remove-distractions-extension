{
  "name": "No Distractions Extension",
  "description": "Base Level Extension",
  "version": "1.0",
  "manifest_version": 3,
  "action": {
    "default_icon": "img/no_youtube.png"
  },
  "permissions": [
    "tabs"
  ],
  "content_scripts": [
    {
      "js": [
        "scripts/main.js"
      ],
      "matches": [
        "https://www.youtube.com/*"
      ]
    }
  ]
}
